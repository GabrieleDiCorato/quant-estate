# Scraper Immobiliare ID Settings - Development Environment
# Prefix: SCRAPER__IMM_ID__

# Base scraper settings
SCRAPER__IMM_ID__MIN_DELAY=1.0
SCRAPER__IMM_ID__MAX_DELAY=3.0
SCRAPER__IMM_ID__HEADLESS=false
SCRAPER__IMM_ID__IMPLICIT_WAIT=10
SCRAPER__IMM_ID__PAGE_LOAD_TIMEOUT=30
SCRAPER__IMM_ID__WINDOW_SIZE=[1366, 768]

# URL settings
SCRAPER__IMM_ID__BASE_URL=https://www.immobiliare.it/
SCRAPER__IMM_ID__USE_SORTING=true
SCRAPER__IMM_ID__SORTING_URL_PARAM=criterio=data&ordine=desc
SCRAPER__IMM_ID__USE_FILTERING=true
SCRAPER__IMM_ID__FILTER_URL_PARAM=noAste=1

# ID scraper specific settings
SCRAPER__IMM_ID__MAX_PAGES=80
SCRAPER__IMM_ID__LISTING_LIMIT=2000
